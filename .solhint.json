{
  "extends": "solhint:default",
  "rules": {
    "code-complexity": ["error", 13],
    "compiler-version": ["error", ">=0.6.0 <0.8.0"],
    "func-visibility": ["error", { "ignoreConstructors": true }],
    "max-line-length": ["error", 150],
    "no-console": "off",
    "not-rely-on-time": "off"
  }
}
